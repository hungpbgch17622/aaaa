<a href="/admin/user/new">Add New User</a>
<h1>Search User</h1>
<form action="/admin/user method="GET">
    <div>
        <label>Name</label>
        <input type="text" name="name" value="<%= searchOptions.name %>">
    </div>
    <button type="submit">Search</button>
</form>
<div>
    <% user.forEach(user =>{ %>
        <a href="/admin/user/<%= user._id %>"><%= user.name %></a>
    <% }) %>
</div>
<a href="/admin">Home</a>